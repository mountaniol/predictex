{
  "functions": {
    "src/backend/**/*.mjs": {
      "runtime": "nodejs18.x"
    }
  },
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/src/backend/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
